#!/usr/bin/env bash
export node_env=hot
node_modules/.bin/webpack-dev-server \
  --devtool cheap-module-eval-source-map \
  --inline \
  --hot \
  --watch \
  --color \
  --output-pathinfo \
  --history-api-fallback \
  --content-base examples/ \
  --output-path / \
  --display-error-details \
  --config webpack.config.hot.js
